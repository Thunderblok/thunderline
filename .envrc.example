# Never commit real tokens. These placeholders are for local use only.

export GITHUB_PERSONAL_ACCESS_TOKEN="ghp_replace_me"
export GITHUB_TOOLSETS=""
export GITHUB_READ_ONLY="1"

# Feature flags
export FEATURE_STREAM_MANAGER="true"

